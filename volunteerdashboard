class VolunteerdashboardModel
    extends FlutterFlowModel<VolunteerdashboardWidget> {
  ///  State fields for stateful widgets in this page.

  // State field(s) for Slider widget.
  double? sliderValue;

  @override
  void initState(BuildContext context) {}

  @override
  void dispose() {}
}
